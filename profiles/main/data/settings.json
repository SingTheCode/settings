{
  "kiroAgent.mcpApprovedEnvVars": ["POSTMAN_API_KEY"],
  "kiroAgent.modelSelection": "claude-opus-4.6",
  "kiroAgent.agentAutonomy": "Autopilot",
  "kiroAgent.trustedTools": ["webFetch", "remote_web_search"],
  "kiroAgent.enableTabAutocomplete": true,
  "kiroAgent.enableCodebaseIndexing": true,
  "kiroAgent.enableDebugLogs": true,
  "kiroAgent.trustedCommands": [
    "git branch --show-current",
    "git *",
    "grep *",
    "npx *",
    "npm *",
    "node *",
    "ls *",
    "find *",
    "python3 *",
    "curl *",
    "head *",
    "wc *",
    "supabase *",
    "yarn *",
    "uv *",
    "test *",
    "docker *",
    "file *",
    "echo *",
    "export *",
    "source *",
    "diff *",
    "sleep *",
    "pwd *",
    "{ git *",
    "cd *",
    "cat *",
    "python *",
    "lsof *",
    "rm *",
    "bash scripts/verify-code-quality.sh",
  ],
  "files.defaultLanguage": "ko",
  "editor.fontSize": 15,
  "editor.fontFamily": "Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "variable": "#9CDCFE",
      "variable.declaration": "#4FC1FF",
      "variable.readonly": "#4EC9B0",
      "parameter": "#FFB86C",
      "property": "#E06C75",
      "property.readonly": "#D19A66",
      "function": "#DCDCAA",
      "function.declaration": "#61AFEF",
      "method": "#DCDCAA",
      "type": "#4EC9B0",
      "class": "#4EC9B0",
      "interface": "#56B6C2",
      "enum": "#C678DD",
      "enumMember": "#D19A66",
      "namespace": "#E5C07B",
    },
  },
  "editor.occurrencesHighlight": "multiFile",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "markdown.occurrencesHighlight.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.applyToAllProfiles": [
    "kiroAgent.trustedTools",
    "kiroAgent.trustedCommands",
    "kiroAgent.enableCodebaseIndexing",
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "search.searchView.semanticSearchBehavior": "auto",
  "python.languageServer": "Default",
  "sync.gist": "9a4a53b7d9002a30fd3380ce7ad42159",
  "git.autofetch": true,
  "git.mergeEditor": true,
  "gitlens.ai.model": "gitkraken",
  "gitlens.ai.gitkraken.model": "gemini:gemini-3.0-pro",
  "terminal.integrated.fontWeight": "normal",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "gitlens.graph.onlyFollowFirstParent": true,
  "window.newWindowProfile": "기본값",
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.suggest.insertTrailingSpace": true,
  "terminal.integrated.suggest.runOnEnter": "always",
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
}
